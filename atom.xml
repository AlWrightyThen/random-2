---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>random</title>
 <link href="http://rjp.github.com/random/atom.xml" rel="self"/>
 <link href="http://rjp.github.com/random/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://rjp.github.com/random/</id>
 <author>
   <name>Dr Nic Williams</name>
   <email>rjpwilliams@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://rjp.github.com/random{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://rjp.github.com/random{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>